    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "healthcare",
    "prescription",
    "ai",
    "fhir",
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "radix-ui",
    "shadcn-ui",
    "vercel"
  ],
  "author": {
    "name": "HealthFlow Development Team",
    "email": "dev@healthflow.egypt.gov",
    "url": "https://healthflow.egypt.gov"
  },
  "license": "MIT",
